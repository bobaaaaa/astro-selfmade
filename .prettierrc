{
	"singleQuote": true,
	"useTabs": true,
	"printWidth": 120,
	"bracketSameLine": true,
	"plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss", "prettier-plugin-astro"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	],
	"importOrder": [
		"<BUILTIN_MODULES>",
		"<THIRD_PARTY_MODULES>",
		"^[./]",
		"",
		"<TYPES>^(node:)",
		"<TYPES>",
		"<TYPES>^[.]"
	],
	"importOrderTypeScriptVersion": "5.4.4"
}
